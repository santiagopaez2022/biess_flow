package ec.gov.iess.creditos.enumeracion;


/**
 * Contiene la situacion de un prestamo.
 * 
 * CESANTE_PRESTAMO_VCIGENTE --> El afiliado esta cesante y el prestamo vigante  
 * ACTIVO_PRESTAMO_ELC_ELF --> El afiliado esta activo y el prestamo en estado ELC o ELF
 *  
 * @version $Revision: 1.1 $  [Sep 21, 2007]
 * @author pablo
 */
public enum SituacionPrestamo {

	CESANTE_PRESTAMO_VIG, CESANTE_PRESTAMO_ELC_ELF, ACTIVO_PRESTAMO_ELC_ELF, ACTIVO_PRESTAMO_VIG_MORA;
	
}