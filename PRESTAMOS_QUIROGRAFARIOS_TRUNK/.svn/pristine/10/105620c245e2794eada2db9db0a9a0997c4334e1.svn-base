public enum TipoPeriodoGracia {
	
	MES_SIGUIENTE(1),MES_SUBSIGUIENTE(2), FIN_DE_MES(3),PERIODO_INTERZAFRA(4);
	
	private int tipoPeriodoGracia;
	
	TipoPeriodoGracia(int tipoPeriodoGracias){
		this.tipoPeriodoGracia = tipoPeriodoGracias;
	}
	
	public int getValor(){
		return this.tipoPeriodoGracia;
	}
	
}