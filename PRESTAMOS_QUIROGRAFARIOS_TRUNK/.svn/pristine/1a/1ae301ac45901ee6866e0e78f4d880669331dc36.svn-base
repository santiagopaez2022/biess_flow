package ec.gov.iess.creditos.modelo.dto;

public class Cuota {
	private int numero;
	private float capital;
	private float interes;
	private float cuota;
	private float seguroSaldos;
	private float dividendo;
	private float saldoCapital;
	
	public Cuota(int numero, float capital, float interes, float cuota,
			float seguroSaldos, float dividendo, float saldoCapital) {
		super();
		this.numero = numero;
		this.capital = capital;
		this.interes = interes;
		this.cuota = cuota;
		this.seguroSaldos = seguroSaldos;
		this.dividendo = dividendo;
		this.saldoCapital = saldoCapital;
	}
	public int getNumero() {
		return numero;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}
	public float getCapital() {
		return capital;
	}
	public void setCapital(float capital) {
		this.capital = capital;
	}
	public float getInteres() {
		return interes;
	}
	public void setInteres(float interes) {
		this.interes = interes;
	}
	public float getCuota() {
		return cuota;
	}
	public void setCuota(float cuota) {
		this.cuota = cuota;
	}
	public float getSeguroSaldos() {
		return seguroSaldos;
	}
	public void setSeguroSaldos(float seguroSaldos) {
		this.seguroSaldos = seguroSaldos;
	}
	public float getDividendo() {
		return dividendo;
	}
	public void setDividendo(float dividendo) {
		this.dividendo = dividendo;
	}
	public float getSaldoCapital() {
		return saldoCapital;
	}
	public void setSaldoCapital(float saldoCapital) {
		this.saldoCapital = saldoCapital;
	}
	
	
}
