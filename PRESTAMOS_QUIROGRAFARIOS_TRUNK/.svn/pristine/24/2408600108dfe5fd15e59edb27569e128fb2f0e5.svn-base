package ec.gov.iess.creditos.dinamico.dto;

import java.io.Serializable;

/**
 * Pojo que contiene los datos de peticion para un contrato
 * 
 * @author Paul.Sampedro <paul.sampedro@biess.fin.ec>
 *
 */
public class DataProveedorRequestDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1234034342240242437L;

	/**
	 * codigoContrato
	 */
	private String codigoContrato;
	/**
	 * codigoEspecial
	 */
	private Long codigoEspecial;
	/**
	 * rucEmpresa
	 */
	private String rucEmpresa;

	public String getCodigoContrato() {
		return codigoContrato;
	}

	public void setCodigoContrato(final String codigoContrato) {
		this.codigoContrato = codigoContrato;
	}

	public Long getCodigoEspecial() {
		return codigoEspecial;
	}

	public void setCodigoEspecial(final Long codigoEspecial) {
		this.codigoEspecial = codigoEspecial;
	}

	public String getRucEmpresa() {
		return rucEmpresa;
	}

	public void setRucEmpresa(final String rucEmpresa) {
		this.rucEmpresa = rucEmpresa;
	}
}
