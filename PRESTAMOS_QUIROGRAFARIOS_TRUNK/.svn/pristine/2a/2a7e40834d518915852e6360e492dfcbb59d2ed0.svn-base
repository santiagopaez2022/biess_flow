/*******************************/
/*       MENU                        */
/*********************************/
#cuadro_menu {
	width: 152px;
	height: 600px;
	margin: 0px 0px 0px 5px;
	padding: 60px 0px 0px 10px;
}

#cuadro_menu {
	background: url(../img/cuadroMenu.jpg);
	background-repeat: no-repeat;
	width: 152px;
	height: auto;
	margin: 40px 0px 0px 5px;
	padding: 21px 0px 0px 10px;
}

#cuadro_menu li {
	list-style-type: none;
	float: left;
	margin: 0px 10px;
	height: 21px;
	font-size: 11px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}

#cuadro_menu li a {
	padding: 0 0 7px 0;
	color: #6B6B6B;
	text-decoration: none;
}

#cuadro_menu li a:hover {
	background: #F3F3F3 url(../img/arrow-on.gif) no-repeat scroll right
		center !important;
	color: #6B6B6B;
	padding-right: 20px;
	padding-top: 5px;
	text-decoration: none;
}

/************************************/
/*       INFORMACION                */
/***********************************/
a.mainlevel:hover,a.mainlevel:active,a.mainlevel:focus,a#active_menu.mainlevel
	{
	background: #F3F3F3 url(../img/arrow-on.gif) no-repeat scroll right
		center !important;
	color: #6B6B6B;
	padding-right: 20px;
	text-decoration: none;
}

a#active_menu.mainlevel {
	font-weight: bold;
}