package ec.gov.iess.creditos.pq.dto;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * Contiene informacion de garantias comprometidas obtenido del SAC
 * 
 * @author hugo.mora
 * @date 2018/08/31
 *
 */
public class GarantiasComprometidasDto implements Serializable {

	private static final long serialVersionUID = -5685979950565000454L;

	private Long operacionSAC;

	private String tipoGarantia;

	private String subtipoGarantia;

	private BigDecimal valorComprometido;

	private Long numeroGarantia;
	
	private Long nut;

	// Getters and setters
	public Long getOperacionSAC() {
		return operacionSAC;
	}

	public void setOperacionSAC(Long operacionSAC) {
		this.operacionSAC = operacionSAC;
	}

	public String getTipoGarantia() {
		return tipoGarantia;
	}

	public void setTipoGarantia(String tipoGarantia) {
		this.tipoGarantia = tipoGarantia;
	}

	public String getSubtipoGarantia() {
		return subtipoGarantia;
	}

	public void setSubtipoGarantia(String subtipoGarantia) {
		this.subtipoGarantia = subtipoGarantia;
	}

	public BigDecimal getValorComprometido() {
		return valorComprometido;
	}

	public void setValorComprometido(BigDecimal valorComprometido) {
		this.valorComprometido = valorComprometido;
	}

	public Long getNumeroGarantia() {
		return numeroGarantia;
	}

	public void setNumeroGarantia(Long numeroGarantia) {
		this.numeroGarantia = numeroGarantia;
	}

	public Long getNut() {
		return nut;
	}

	public void setNut(Long nut) {
		this.nut = nut;
	}

}
