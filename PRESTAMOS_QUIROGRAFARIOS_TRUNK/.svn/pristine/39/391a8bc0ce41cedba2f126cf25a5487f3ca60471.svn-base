package ec.gov.iess.pq.concesion.web.enumeration;

/**
 * Enumeracion para almacenar parametros generales para reemplazo de textos
 *
 * @author mauricio.toapanta
 * @date 2020/06/14
 *
 */
public enum ParametrosGeneralesEnum {

	PQW_FORM_DATOS_IESS("PQW_FORM_DATOS_IESS","string","${PQW_FORM_DATOS_IESS}"),
	PQW_CORREO_CONCESION("PQW_CORREO_CONCESION","string", "${PQW_CORREO_CONCESION}"),
	PQW_TRAMITE_IESS("PQW_TRAMITE_IESS","string","${PQW_TRAMITE_IESS}");


	private String codigo;
	private String tipoDato;
	private String valorReemplazoTexto;

	private ParametrosGeneralesEnum(String codigo, String tipoDato, String valorReemplazoTexto) {
		this.codigo = codigo;
		this.tipoDato = tipoDato;
		this.valorReemplazoTexto = valorReemplazoTexto;
	}

	public String getCodigo() {
		return codigo;
	}

	public String getTipoDato() {
		return tipoDato;
	}

	public String getValorReemplazoTexto() {
		return valorReemplazoTexto;
	}
}
