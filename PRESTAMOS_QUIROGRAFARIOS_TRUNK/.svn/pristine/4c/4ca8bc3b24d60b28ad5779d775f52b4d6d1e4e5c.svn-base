package ec.gov.iess.creditos.constante;

/**
 * Constantes para tipos de miembros del proyecto
 * 
 * @author jsanchez
 * @version 1.0
 */
public enum TipoMiembroFideicomiso {

	SOLICITANTE("S"), CONSTRUCTOR("C"), MIEMBRO("M"), SOLICITANTE_CONSTRUCTOR("SC");

	public final String tipo;

	TipoMiembroFideicomiso(String tipo) {
		this.tipo = tipo;
	}

	public String tipo() {
		return this.tipo;
	}
}
