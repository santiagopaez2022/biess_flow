package ec.gov.iess.creditos.focalizados.dto;

import java.io.Serializable;

/**
 * Dto para respuesta de consulta al catalogo de punto de venta y productos
 * 
 * @author hugo.mora
 *
 */
public class PuntoVentaProductoDto implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer codigoMEER;

	private Integer codigoProveedorMEER;

	private Integer codigoPuntoVentaMEER;

	private Integer codigoProductoMEER;

	private String estado;

	// Getters and setters
	public Integer getCodigoMEER() {
		return codigoMEER;
	}

	public void setCodigoMEER(Integer codigoMEER) {
		this.codigoMEER = codigoMEER;
	}

	public Integer getCodigoProveedorMEER() {
		return codigoProveedorMEER;
	}

	public void setCodigoProveedorMEER(Integer codigoProveedorMEER) {
		this.codigoProveedorMEER = codigoProveedorMEER;
	}

	public Integer getCodigoPuntoVentaMEER() {
		return codigoPuntoVentaMEER;
	}

	public void setCodigoPuntoVentaMEER(Integer codigoPuntoVentaMEER) {
		this.codigoPuntoVentaMEER = codigoPuntoVentaMEER;
	}

	public Integer getCodigoProductoMEER() {
		return codigoProductoMEER;
	}

	public void setCodigoProductoMEER(Integer codigoProductoMEER) {
		this.codigoProductoMEER = codigoProductoMEER;
	}

	public String getEstado() {
		return estado;
	}

	public void setEstado(String estado) {
		this.estado = estado;
	}

}
