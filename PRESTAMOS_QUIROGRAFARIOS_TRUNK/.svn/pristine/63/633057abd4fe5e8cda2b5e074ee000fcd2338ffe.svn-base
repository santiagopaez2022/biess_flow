package ec.gov.iess.creditos.pq.excepcion;

/**
 * 
 * <b>
 * Excepcion para el manejo de proveedores.
 * </b>
 *  
 * @author cbastidas
 * @version $Revision: 1.2 $ <p>[$Author: smanosalvas $, $Date: 2011/05/03 13:29:24 $]</p>
 */
public class ProveedorException extends Exception {
 
	private static final long serialVersionUID = -7229270294156893359L;

	public ProveedorException() {
	}

	public ProveedorException(String message) {
		super(message);
	}

	public ProveedorException(Throwable cause) {
		super(cause);
	}

	public ProveedorException(String message, Throwable cause) {
		super(message, cause);
	}

}
