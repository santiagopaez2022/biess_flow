package ec.gov.iess.creditos.pq.dto;

import java.math.BigDecimal;

/**
 * 
 * @author Paul.Sampedro <paul.sampedro@biess.fin.ec>
 *
 */
public class PrestamoRequestDto {

	private BigDecimal monto;
	private int plazo;
	private String fechaPrimerVencimiento;
	private String tipoTablaAmortizacion;
	private BigDecimal montoCuota;
	private boolean cargaDividendos;

	public BigDecimal getMonto() {
		return monto;
	}

	public void setMonto(BigDecimal monto) {
		this.monto = monto;
	}

	public int getPlazo() {
		return plazo;
	}

	public void setPlazo(int plazo) {
		this.plazo = plazo;
	}

	public String getFechaPrimerVencimiento() {
		return fechaPrimerVencimiento;
	}

	public void setFechaPrimerVencimiento(String fechaPrimerVencimiento) {
		this.fechaPrimerVencimiento = fechaPrimerVencimiento;
	}

	public String getTipoTablaAmortizacion() {
		return tipoTablaAmortizacion;
	}

	public void setTipoTablaAmortizacion(String tipoTablaAmortizacion) {
		this.tipoTablaAmortizacion = tipoTablaAmortizacion;
	}

	public BigDecimal getMontoCuota() {
		return montoCuota;
	}

	public void setMontoCuota(BigDecimal montoCuota) {
		this.montoCuota = montoCuota;
	}

	public boolean isCargaDividendos() {
		return cargaDividendos;
	}

	public void setCargaDividendos(boolean cargaDividendos) {
		this.cargaDividendos = cargaDividendos;
	}
}
