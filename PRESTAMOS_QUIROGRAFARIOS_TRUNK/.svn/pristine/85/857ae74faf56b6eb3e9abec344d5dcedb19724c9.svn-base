public enum EstadoPrestamoEnum {
	VIG("Vigente"), 
	NAC("Non-Accrual"), 
	VCD("En Mora"), 
	CAS("Castigada"), 
	CAN("Cancelado"),
	AFE("Por Vencer"),          
    CAP("Capitalizado"),
    GEN("Generado"),
    PND("Por Vencer"),
    PRE("Cancelado"),      
    VCN("En Mora"),
    VCR("En Mora"),
	EPL("En Planilla"); 

	private String estado;

	private EstadoPrestamoEnum(String estado) {
		this.estado = estado;
	}

	public String getEstado() {
		return estado;
	}
}
