package ec.gov.iess.creditos.modelo.dto;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class SucursalDto implements Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 1L;

	@Id
	private String nombre = "";

	private String direccion = "";

	private String telefono = "";

	private String cedulaRepLegal = "";

	private String nombreRepLegal = "";

	private String telefonoRepLegal = "";

	private String rucInstFinanciera = "";

	private String numeroCuenta = "";

	private String tipoCuenta = "";

	private String codDivisionPolitica="";

	/**
	 * @return the nombre
	 */
	public String getNombre() {
		return nombre;
	}

	/**
	 * @param nombre
	 *            the nombre to set
	 */
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	/**
	 * @return the direccion
	 */
	public String getDireccion() {
		return direccion;
	}

	/**
	 * @param direccion
	 *            the direccion to set
	 */
	public void setDireccion(String direccion) {
		this.direccion = direccion;
	}

	/**
	 * @return the telefono
	 */
	public String getTelefono() {
		return telefono;
	}

	/**
	 * @param telefono
	 *            the telefono to set
	 */
	public void setTelefono(String telefono) {
		this.telefono = telefono;
	}

	/**
	 * @return the cedulaRepLegal
	 */
	public String getCedulaRepLegal() {
		return cedulaRepLegal;
	}

	/**
	 * @param cedulaRepLegal
	 *            the cedulaRepLegal to set
	 */
	public void setCedulaRepLegal(String cedulaRepLegal) {
		this.cedulaRepLegal = cedulaRepLegal;
	}

	/**
	 * @return the nombreRepLegal
	 */
	public String getNombreRepLegal() {
		return nombreRepLegal;
	}

	/**
	 * @param nombreRepLegal
	 *            the nombreRepLegal to set
	 */
	public void setNombreRepLegal(String nombreRepLegal) {
		this.nombreRepLegal = nombreRepLegal;
	}

	/**
	 * @return the telefonoRepLegal
	 */
	public String getTelefonoRepLegal() {
		return telefonoRepLegal;
	}

	/**
	 * @param telefonoRepLegal
	 *            the telefonoRepLegal to set
	 */
	public void setTelefonoRepLegal(String telefonoRepLegal) {
		this.telefonoRepLegal = telefonoRepLegal;
	}

	/**
	 * @return the rucInstFinanciera
	 */
	public String getRucInstFinanciera() {
		return rucInstFinanciera;
	}

	/**
	 * @param rucInstFinanciera
	 *            the rucInstFinanciera to set
	 */
	public void setRucInstFinanciera(String rucInstFinanciera) {
		this.rucInstFinanciera = rucInstFinanciera;
	}

	/**
	 * @return the numeroCuenta
	 */
	public String getNumeroCuenta() {
		return numeroCuenta;
	}

	/**
	 * @param numeroCuenta
	 *            the numeroCuenta to set
	 */
	public void setNumeroCuenta(String numeroCuenta) {
		this.numeroCuenta = numeroCuenta;
	}

	/**
	 * @return the tipoCuenta
	 */
	public String getTipoCuenta() {
		return tipoCuenta;
	}

	/**
	 * @param tipoCuenta
	 *            the tipoCuenta to set
	 */
	public void setTipoCuenta(String tipoCuenta) {
		this.tipoCuenta = tipoCuenta;
	}

	public String getCodDivisionPolitica() {
		return codDivisionPolitica;
	}

	public void setCodDivisionPolitica(String codDivisionPolitica) {
		this.codDivisionPolitica = codDivisionPolitica;
	}

}
