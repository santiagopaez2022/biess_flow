package ec.gov.iess.creditos.focalizados.dto;

import java.io.Serializable;

/**
 * Pagina de request para codificacion MEER de proveedores y puntos de venta
 * 
 * @author hugo.mora
 *
 */
public class CodigoMeerFocalizadoRequestDto implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer codigoProveedorMeer;

	private Integer codigoPuntoVentaMeer;

	// Getters and setters
	public Integer getCodigoProveedorMeer() {
		return codigoProveedorMeer;
	}

	public void setCodigoProveedorMeer(Integer codigoProveedorMeer) {
		this.codigoProveedorMeer = codigoProveedorMeer;
	}

	public Integer getCodigoPuntoVentaMeer() {
		return codigoPuntoVentaMeer;
	}

	public void setCodigoPuntoVentaMeer(Integer codigoPuntoVentaMeer) {
		this.codigoPuntoVentaMeer = codigoPuntoVentaMeer;
	}

}
