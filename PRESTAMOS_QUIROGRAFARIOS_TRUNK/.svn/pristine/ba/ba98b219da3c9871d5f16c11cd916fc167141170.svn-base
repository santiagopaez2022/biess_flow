package ec.gov.iess.creditos.modelo.dto;

import ec.gov.iess.creditos.modelo.persistencia.Prestamo;
import ec.gov.iess.creditos.modelo.persistencia.PrestamoInformacion;

import java.io.Serializable;
import java.util.Date;


public class SolicitudPda implements Serializable {
	
	private static final long serialVersionUID = -8787498580400126270L;
	
	private String apenomrepleg;
	private String dirsuc;
	private String nomdivpol;
	private String nomdivpolDP;
	private String telsuc;
	private Date fechactabancaria;
	private String primeravez;
	private Prestamo prestamo;
	private String tipoCuentaAnterior;
	private String numeroCuentaAnterior;
	private String entidadFinancieraAnterior;
	private Date fechaRegistroAnterior;
	// INC-2148: Refugiados.
	private String tipoBeneficiario;
	//INC-2286: Ferrocarriles
	private PrestamoInformacion prestamoInformacion;
	
	private boolean datoSeleccionado;
	
	public String getTipoCuentaAnterior() {
		return tipoCuentaAnterior;
	}

	public void setTipoCuentaAnterior(String tipoCuentaAnterior) {
		this.tipoCuentaAnterior = tipoCuentaAnterior;
	}

	public String getNumeroCuentaAnterior() {
		return numeroCuentaAnterior;
	}

	public void setNumeroCuentaAnterior(String numeroCuentaAnterior) {
		this.numeroCuentaAnterior = numeroCuentaAnterior;
	}

	public String getEntidadFinancieraAnterior() {
		return entidadFinancieraAnterior;
	}

	public void setEntidadFinancieraAnterior(String entidadFinancieraAnterior) {
		this.entidadFinancieraAnterior = entidadFinancieraAnterior;
	}

	public String getApenomrepleg() {
		return this.apenomrepleg;
	}

	public void setApenomrepleg(String apenomrepleg) {
		this.apenomrepleg = apenomrepleg;
	}

	public String getDirsuc() {
		return this.dirsuc;
	}

	public void setDirsuc(String dirsuc) {
		this.dirsuc = dirsuc;
	}

	public String getNomdivpol() {
		return this.nomdivpol;
	}

	public void setNomdivpol(String nomdivpol) {
		this.nomdivpol = nomdivpol;
	}

	public String getNomdivpolDP() {
		return this.nomdivpolDP;
	}

	public void setNomdivpolDP(String nomdivpolDP) {
		this.nomdivpolDP = nomdivpolDP;
	}

	public String getTelsuc() {
		return this.telsuc;
	}

	public void setTelsuc(String telsuc) {
		this.telsuc = telsuc;
	}

	public Date getFechactabancaria() {
		return this.fechactabancaria;
	}

	public void setFechactabancaria(Date fechactabancaria) {
		this.fechactabancaria = fechactabancaria;
	}

	public String getPrimeravez() {
		return this.primeravez;
	}

	public void setPrimeravez(String primeravez) {
		this.primeravez = primeravez;
	}

	public Prestamo getPrestamo() {
		return this.prestamo;
	}

	public void setPrestamo(Prestamo pretamo) {
		this.prestamo = pretamo;
	}

	public void setFechaRegistroAnterior(Date fechaRegistroAnterior) {
		this.fechaRegistroAnterior = fechaRegistroAnterior;
	}

	public Date getFechaRegistroAnterior() {
		return fechaRegistroAnterior;
	}

	/**
	 * @return the tipoBeneficiario
	 */
	public String getTipoBeneficiario() {
		return tipoBeneficiario;
	}

	/**
	 * @param tipoBeneficiario the tipoBeneficiario to set
	 */
	public void setTipoBeneficiario(String tipoBeneficiario) {
		this.tipoBeneficiario = tipoBeneficiario;
	}

	/**
	 * @return {@link PrestamoInformacion}
	 */
	public PrestamoInformacion getPrestamoInformacion() {
		return prestamoInformacion;
	}

	/**
	 * @param prestamoInformacion
	 */
	public void setPrestamoInformacion(PrestamoInformacion prestamoInformacion) {
		this.prestamoInformacion = prestamoInformacion;
	}

	public boolean isDatoSeleccionado() {
		return datoSeleccionado;
	}

	public void setDatoSeleccionado(boolean datoSeleccionado) {
		this.datoSeleccionado = datoSeleccionado;
	}
	
}