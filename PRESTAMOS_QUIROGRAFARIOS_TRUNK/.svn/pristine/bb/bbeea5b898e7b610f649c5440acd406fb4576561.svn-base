package ec.fin.biess.creditos.pq.modelo.dto;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * Calse dto para almacenar informacion de cesantias de un asegurado
 * @author edison.cayambe
 *
 */
public class InformacionCesantia implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private BigDecimal valorCesantia;
	private String identificacion;

	// Metodos SET y GET

	public BigDecimal getValorCesantia() {
		return valorCesantia;
	}

	public void setValorCesantia(BigDecimal valorCesantia) {
		this.valorCesantia = valorCesantia;
	}

	public String getIdentificacion() {
		return identificacion;
	}

	public void setIdentificacion(String identificacion) {
		this.identificacion = identificacion;
	}
}
