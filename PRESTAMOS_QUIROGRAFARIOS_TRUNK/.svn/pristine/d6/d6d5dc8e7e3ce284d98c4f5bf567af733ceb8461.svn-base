package ec.gov.iess.creditos.enumeracion;

/**
 * Enum para manejar los tipos de documento para la identificacion de una
 * persona
 * 
 * @author Daniel Cardenas
 * 
 */
public enum TipoDocumento {

	CEDULA("CED"), PASAPORTE("PAS"), RUC("RUC");

	String codigo;

	private TipoDocumento(String codigo) {
		this.codigo = codigo;
	}

	/**
	 * @return the codigo
	 */
	public String getCodigo() {
		return codigo;
	}
}