package ec.gov.iess.creditos.pq.dto;

import java.io.Serializable;

/**
 * Dto de respuesta para obtener informacion de garantias desde SAC
 * 
 * @author hugo.mora
 * @date 2018/08/31
 *
 */
public class InformacionGarantiasResponseDto implements Serializable {

	private static final long serialVersionUID = -2235648002164791046L;

	private InformacionGarantias informacionGarantiasDto;

	private String codigoRespuesta;

	private String mensajeRespuesta;

	// Getters and setters
	public InformacionGarantias getInformacionGarantiasDto() {
		return informacionGarantiasDto;
	}

	public void setInformacionGarantiasDto(InformacionGarantias informacionGarantiasDto) {
		this.informacionGarantiasDto = informacionGarantiasDto;
	}

	public String getCodigoRespuesta() {
		return codigoRespuesta;
	}

	public void setCodigoRespuesta(String codigoRespuesta) {
		this.codigoRespuesta = codigoRespuesta;
	}

	public String getMensajeRespuesta() {
		return mensajeRespuesta;
	}

	public void setMensajeRespuesta(String mensajeRespuesta) {
		this.mensajeRespuesta = mensajeRespuesta;
	}

}
