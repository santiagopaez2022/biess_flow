	private static final long serialVersionUID = 1L;

	public CodigoReservaPaqueteTuristicoException() {
		super();
	}

	public CodigoReservaPaqueteTuristicoException(final String arg0, final Throwable arg1) {
		super(arg0, arg1);
	}

	public CodigoReservaPaqueteTuristicoException(final String arg0) {
