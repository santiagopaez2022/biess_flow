package ec.gov.iess.creditos.excepcion;

import javax.ejb.ApplicationException;

/**
 * Excepcion Query Generico
 * 28/08/2010: rituana inc-5780 agregando @ApplicationException
 * @version 1.0
 * @author rtituana
 */
@ApplicationException(rollback = true)
public class QueryGenericoException  extends Exception{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Contructor 
	 * @author rtituana
	 */
	public QueryGenericoException() {
		super();
	}

	/**
	 * Contructor 
	 * @param arg0
	 * @author rtituana
	 */
	public QueryGenericoException(String arg0) {
		super(arg0);
		
	}

	/**
	 * Contructor 
	 * @param arg0
     * @author rtituana
	 */
	public QueryGenericoException(Throwable arg0) {
		super(arg0);
		
	}

	/**
	 * Contructor 
	 * @param arg0
	 * @param arg1
     * @author rtituana
	 */
	public QueryGenericoException(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

}
