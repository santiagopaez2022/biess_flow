package ec.gov.iess.creditos.focalizados.dto;

import java.io.Serializable;
import java.util.List;

/**
 * Dto para respuesta de consulta al catalogo de prestamos pq focalizados
 * 
 * @author hugo.mora
 *
 * @param <T>
 */
public class CatalogoFocalizadoResponseDto<T> implements Serializable {

	private static final long serialVersionUID = 1L;

	private List<T> listaCatalogos;

	private String codigo;

	private String mensaje;

	private T catalogo;

	// Getters and setters
	public List<T> getListaCatalogos() {
		return listaCatalogos;
	}

	public void setListaCatalogos(List<T> listaCatalogos) {
		this.listaCatalogos = listaCatalogos;
	}

	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	public String getMensaje() {
		return mensaje;
	}

	public void setMensaje(String mensaje) {
		this.mensaje = mensaje;
	}

	public T getCatalogo() {
		return catalogo;
	}

	public void setCatalogo(T catalogo) {
		this.catalogo = catalogo;
	}

}
