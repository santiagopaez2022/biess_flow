package ec.gov.iess.creditos.excepcion;

import javax.ejb.ApplicationException;

/**
 * Excepcion para periodo de vigencia de generacion de comprobantes de pago
 * 
 * @author hugo.mora
 * @date 2016/08/03
 *
 */
@ApplicationException(rollback = true)
public class PeriodoComprobanteException extends Exception {

	private static final long serialVersionUID = 1L;

	public PeriodoComprobanteException() {
	}

	public PeriodoComprobanteException(String message) {
		super(message);
	}

	public PeriodoComprobanteException(Throwable cause) {
		super(cause);
	}

	public PeriodoComprobanteException(String message, Throwable cause) {
		super(message, cause);
	}

}
