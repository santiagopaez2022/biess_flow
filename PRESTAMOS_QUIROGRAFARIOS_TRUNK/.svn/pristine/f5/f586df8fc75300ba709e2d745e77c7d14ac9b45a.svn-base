package ec.fin.biess.creditos.pq.enumeracion;

/**
 * Enumeracion para tipos de empleador usado en la generacion de comprobantes y liquidacion anticipada
 * 
 * @author hugo.mora
 * @date 2016/08/05
 *
 */
public enum TipoEmpleadorEnum {

	PRI("PRI", "Privados"), PUB("PUB", "Publicos"), MIN("MIN", "Ministerio Relaciones Exteriores"), JUB("JUB","Jubilados");

	private TipoEmpleadorEnum(String codigo, String descripcion) {
		this.codigo = codigo;
		this.descripcion = descripcion;
	}

	private String codigo;
	private String descripcion;

	// Getters and setters

	public String getCodigo() {
		return codigo;
	}

	public String getDescripcion() {
		return descripcion;
	}

}