package ec.fin.biess.creditos.pq.modelo.dto;

import java.math.BigDecimal;
import java.util.List;

/**
 * Clase para almacenar la informacion obteneida de prestaciones de jubilados/pensionistas.
 * 
 * @author diego.iza
 * 
 */
public class InformacionPrestacionPensionado {

	/**
	 * totalIngresos.
	 */
	private BigDecimal totalIngresos;

	/**
	 * totalEgresos.
	 */
	private BigDecimal totalEgresos;

	/**
	 * valorAComprometer.
	 */
	private BigDecimal valorAComprometer;

	/**
	 * listaPrestaciones.
	 */
	private List<PrestacionPensionado> listaPrestaciones;

	/**
	 * Constructor.
	 * 
	 * @param totalIngresos
	 * @param totalEgresos
	 * @param valorAComprometer
	 * @param listaPrestaciones
	 */
	public InformacionPrestacionPensionado(BigDecimal totalIngresos, BigDecimal totalEgresos,
			BigDecimal valorAComprometer, List<PrestacionPensionado> listaPrestaciones) {
		super();
		this.totalIngresos = totalIngresos;
		this.totalEgresos = totalEgresos;
		this.valorAComprometer = valorAComprometer;
		this.listaPrestaciones = listaPrestaciones;
	}

	/**
	 * @return the totalIngresos
	 */
	public BigDecimal getTotalIngresos() {
		return totalIngresos;
	}

	/**
	 * @param totalIngresos
	 *            the totalIngresos to set
	 */
	public void setTotalIngresos(BigDecimal totalIngresos) {
		this.totalIngresos = totalIngresos;
	}

	/**
	 * @return the totalEgresos
	 */
	public BigDecimal getTotalEgresos() {
		return totalEgresos;
	}

	/**
	 * @param totalEgresos
	 *            the totalEgresos to set
	 */
	public void setTotalEgresos(BigDecimal totalEgresos) {
		this.totalEgresos = totalEgresos;
	}

	/**
	 * @return the valorAComprometer
	 */
	public BigDecimal getValorAComprometer() {
		return valorAComprometer;
	}

	/**
	 * @param valorAComprometer
	 *            the valorAComprometer to set
	 */
	public void setValorAComprometer(BigDecimal valorAComprometer) {
		this.valorAComprometer = valorAComprometer;
	}

	/**
	 * @return the listaPrestaciones
	 */
	public List<PrestacionPensionado> getListaPrestaciones() {
		return listaPrestaciones;
	}

	/**
	 * @param listaPrestaciones
	 *            the listaPrestaciones to set
	 */
	public void setListaPrestaciones(List<PrestacionPensionado> listaPrestaciones) {
		this.listaPrestaciones = listaPrestaciones;
	}

}
