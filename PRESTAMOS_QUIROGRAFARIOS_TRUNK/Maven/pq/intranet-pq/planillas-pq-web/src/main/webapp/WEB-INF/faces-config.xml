  <managed-bean-class>ec.gov.iess.planillaspq.web.bean.DatosBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <!-- Manejador de consultas de creditos -->
 <managed-bean>
  <managed-bean-name>consultaCredito</managed-bean-name>
  <managed-bean-class>ec.gov.iess.planillaspq.web.backing.ConsultaCreditoBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
