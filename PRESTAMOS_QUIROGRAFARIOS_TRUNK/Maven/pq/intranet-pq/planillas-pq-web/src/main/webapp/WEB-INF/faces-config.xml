	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/pages/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>salir</from-outcome>
			<to-view-id>/pages/salir.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/ajustarplanillahl.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscarplanilla</from-outcome>
			<to-view-id>/pages/ajustarplanillahlaccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/ajustarplanillahost.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscarplanillahost</from-outcome>
			<to-view-id>/pages/ajustarplanillahostaccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/anularplanillahost.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscarplanillahost</from-outcome>
			<to-view-id>/pages/anularplanillahostaccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
