		<from-view-id>/pages/anularplanillahl.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscarplanillahl</from-outcome>
			<to-view-id>/pages/anularplanillahlaccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/ordenes/consultarOrdenes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>imprimirConsulta</from-outcome>
			<to-view-id>/pages/ordenes/imprimirConsultaOrdenes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/aprobacion/cargarcedulasmasivas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aprobarMasivo</from-outcome>
			<to-view-id>/pages/aprobacion/aprobarsolicitudesmasivas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/aprobacion/cargarcedulasmasivaspdv.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aprobarMasivoPdv</from-outcome>
			<to-view-id>/pages/aprobacion/aprobarsolicitudesmasivaspdv.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<navigation-case>
			<from-outcome>consultarCreditos</from-outcome>
			<to-view-id>/pages/comprobantesLiquidacion/creditos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>principalSalir</from-outcome>
			<to-view-id>/pages/principal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
	    	<description>Cualquier accion que retorne "pagIndSeleccion" envia a
						la pagina /comprobantesLiquidacion/pagIndSeleccion.xhtml</description>
			<from-outcome>pagIndSeleccion</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/pagIndSeleccion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		   	<description>Cualquier accion que retorne "pagIndSeleccionSaldos"
						envia a la pagina /comprobantesLiquidacion/pagIndSeleccionSaldos.xhtml</description>
		   	<from-outcome>pagIndSeleccionSaldos</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/pagIndSeleccionSaldos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		   	<description>Cualquier accion que retorne "comprobantesPago" envia a
						la pagina /comprobantesLiquidacion/comprobantesPago.xhtml</description>
		   	<from-outcome>comprobantesPago</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/comprobantesPago.xhtml</to-view-id>
		</navigation-case>
	  	<navigation-case>
			<description>Cualquier accion que retorne "calculoLiquidacion" envia
						a la pagina /comprobantesLiquidacion/calculoLiquidacion.xhtml</description>
		   	<from-outcome>calculoLiquidacion</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/calculoLiquidacion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
		   	<description>Cualquier accion que retorne "resultadoLiquidacion"
						envia a la pagina /comprobantesLiquidacion/resultadoLiquidacion.xhtml</description>
		   	<from-outcome>resultadoLiquidacion</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/resultadoLiquidacion.xhtml</to-view-id>
	 	</navigation-case>
		<navigation-case>
		   	<description>Cualquier accion que retorne "detallePrestamo" envia a
						la pagina /detallePrestamo.xhtml</description>
		   	<from-outcome>detallePrestamo</from-outcome>
		   	<to-view-id>/pages/comprobantesLiquidacion/detallePrestamo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>comprobanteDividendos</from-outcome>
			<to-view-id>/pages/comprobantesLiquidacion/comprobantePagoDividendos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>consultaComprobanteAnulacion</from-outcome>
			<to-view-id>/pages/comprobantesLiquidacion/comprobantePagoDividendos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>liquidacionesPago</from-outcome>
			<to-view-id>/pages/comprobantesLiquidacion/liquidacionesPago.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<resource-bundle>
			<base-name>ec.gov.iess.planillaspq.web.resources.titulos</base-name>
			<var>titulos</var>
		</resource-bundle>
		<resource-bundle>
